 {\r\n    width: 770px;\r\n    height: 55px;\r\n    box-sizing: border-box;\r\n    display: inline-block;\r\n    background-color: rgb(255, 255, 255);\r\n    margin-bottom: 2px;\r\n}\r\n\r\n.date {\r\n    width: 75px;\r\n    height: 15px;\r\n    box-sizing: border-box;\r\n    display: inline-block;\r\n    color: gray;\r\n    padding-left: 15px;\r\n    position: relative;\r\n    top: 3px;\r\n}\r\n\r\n.username {\r\n    width: 95px;\r\n    height: 15px;\r\n    display: inline-block;\r\n    box-sizing: border-box;\r\n    position: relative;\r\n    top: 3px;\r\n}\r\n\r\n.subject {\r\n    width: 175px;\r\n    height: 15px;\r\n    display: inline-block;\r\n    position: relative;\r\n    top: 4px;\r\n}\r\n\r\n.contentMail {\r\n    width: 278px;\r\n    height: 15px;\r\n    display: inline-block;\r\n    color: gray;\r\n    font-weight: 400;\r\n    position: relative;\r\n    top: 5px;\r\n}\r\n\r\n.favImg,.favedImg {\r\n    background: url(./star.svg) no-repeat center;\r\n    width: 60px;\r\n    height: 25px;\r\n    margin: 0 3px 0 10px;\r\n    position: relative;\r\n    top: 10px;\r\n    display: inline-block;\r\n    cursor:  pointer;\r\n    border: none;\r\n    \r\n}\r\n\r\n.favedImg {\r\n    background: url(./starFilled.svg) no-repeat center;\r\n}\r\n\r\n.deleteImg{\r\n    background: url(./delete.svg) no-repeat center; \r\n    width: 40px;\r\n    height: 23px;\r\n    position: relative;\r\n    top: 9px;\r\n    display: inline-block;\r\n    cursor: pointer;\r\n    border: none;\r\n}\r\n\r\n.overlay {\r\n    position: fixed;\r\n    width: 100%;\r\n    height: 100%;\r\n    margin: 0;\r\n    top: 0;\r\n    left: 0;\r\n    visibility: hidden;\r\n    color: white;\r\n    background-color: rgba(5, 10, 30, 0.5);\r\n}\r\n\r\n.dialog {\r\n    position: absolute;\r\n    top: 175px;\r\n    left: 456px;\r\n    width: 450px;\r\n    height: 200px;\r\n    margin: 0;\r\n    visibility: hidden;\r\n    z-index: 997;\r\n    background-color: white;\r\n    border-radius: 5px;\r\n}\r\n\r\n.dialogTitle {\r\n    box-sizing: border-box;\r\n    position: absolute;\r\n    left: 80px;\r\n    top: 30px;\r\n    font-size: 22px;\r\n}\r\n\r\n.deleteMail {\r\n    border: 1px solid silver;\r\n    width: 150px;\r\n    height: 50px;\r\n    box-sizing: border-box;\r\n    position: absolute;\r\n    top: 107px;\r\n    left: 60px;\r\n    font-size: 18px;\r\n    border-radius: 5px; \r\n}\r\n\r\n.dontDeleteMail {\r\n    border: 1px solid silver;\r\n    width: 150px;\r\n    height: 50px;\r\n    box-sizing: border-box;\r\n    position: absolute;\r\n    top: 107px;\r\n    left: 246px;\r\n    font-size: 18px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.deleteMail:hover, .dontDeleteMail:hover {\r\n    background-color: #0591FD;\r\n    color: white;\r\n}\r\n\r\n\r\nbody.dialog-visible .overlay,body.dialog-visible .dialog { /*body'de dialog-visible varsa overlay görünür olucak.*/\r\n    visibility: visible;\r\n}\r\n\r\n.error {\r\n    width: 200px;\r\n    height: 50px;\r\n    position: absolute;\r\n    top: 200px;\r\n    left: 300px;\r\n    \r\n}\r\n"},"lineCount":null}},"error":null,"hash":"101f4c90de46272645877c429d729862","cacheData":{"env":{}}}